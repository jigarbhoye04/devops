# Analytics Service - Environment Variables Example
# Copy this file to .env and update with your values

# Django Settings
DJANGO_SECRET_KEY=your-secret-key-here-change-in-production
DEBUG=True
ALLOWED_HOSTS=*
LOG_LEVEL=INFO

# Database Configuration
DB_NAME=helios_analytics
DB_USER=postgres
DB_PASSWORD=your-db-password
DB_HOST=localhost
DB_PORT=5432

# Kafka Configuration
KAFKA_BROKERS=localhost:9092
KAFKA_TOPIC_AUCTION_OUTCOMES=auction_outcomes
KAFKA_CONSUMER_GROUP=analytics-service-group

# Optional: Django Superuser (for auto-creation)
# DJANGO_SUPERUSER_USERNAME=admin
# DJANGO_SUPERUSER_EMAIL=admin@example.com
# DJANGO_SUPERUSER_PASSWORD=admin
